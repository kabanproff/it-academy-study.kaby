_.extend({},flexblock.style["l"],flexblock.style["m"],flexblock.style["s"],flexblock.style["xs"]);if(size=="xxs")this_size_options=_.extend({},flexblock.style["l"],flexblock.style["m"],flexblock.style["s"],flexblock.style["xs"],flexblock.style["xxs"]);return this_size_options}function playAnimation(flexblock,size,id,index){let this_size_options=getStylesBySize(flexblock,size);let animateType=_.get(this_size_options,"animateType")||"off";let animateEvent=_.get(this_size_options,"animateEvent")||"blockVisible";
let animateDelay=_.get(this_size_options,"animateDelay")||0;let animateDuration=_.get(this_size_options,"animateDuration")||300;let animateDistance=_.get(this_size_options,"animateDistance")||100;let animateZoom=_.get(this_size_options,"animateZoom")||1;let animateEasing=_.get(this_size_options,"animateEasing")||"linear";let animateRotate=_.get(this_size_options,"animateRotate")||0;if(animateType=="fadeIn")anime({targets:"#b_"+id+' .flexblock_wrapper[data-index="'+index+'"]',keyframes:[{opacity:0,
rotate:animateRotate,scale:animateZoom,duration:0},{opacity:1,rotate:0,scale:1,duration:animateDuration,delay:animateDelay}],easing:animateEasing});if(animateType=="fadeInLeft")anime({targets:"#b_"+id+' .flexblock_wrapper[data-index="'+index+'"]',keyframes:[{opacity:0,rotate:animateRotate,scale:animateZoom,translateX:"-"+animateDistance,duration:0},{opacity:1,rotate:0,scale:1,translateX:0,duration:animateDuration,delay:animateDelay}],easing:animateEasing});if(animateType=="fadeInRight")anime({targets:"#b_"+
id+' .flexblock_wrapper[data-index="'+index+'"]',keyframes:[{opacity:0,rotate:animateRotate,scale:animateZoom,translateX:animateDistance,duration:0},{opacity:1,rotate:0,scale:1,translateX:0,duration:animateDuration,delay:animateDelay}],easing:"linear"});if(animateType=="fadeInTop")anime({targets:"#b_"+id+' .flexblock_wrapper[data-index="'+index+'"]',keyframes:[{opacity:0,rotate:animateRotate,scale:animateZoom,translateY:"-"+animateDistance,duration:0},{opacity:1,rotate:0,scale:1,translateY:0,duration:animateDuration,
delay:animateDelay}],easing:animateEasing});if(animateType=="fadeInBottom")anime({targets:"#b_"+id+' .flexblock_wrapper[data-index="'+index+'"]',keyframes:[{opacity:0,rotate:animateRotate,scale:animateZoom,translateY:animateDistance,duration:0},{opacity:1,rotate:0,scale:1,translateY:0,duration:animateDuration,delay:animateDelay}],easing:animateEasing});if(animateType=="zoomIn")anime({targets:"#b_"+id+' .flexblock_wrapper[data-index="'+index+'"]',keyframes:[{opacity:0,rotate:animateRotate,scale:animateZoom,
duration:0},{opacity:1,rotate:0,scale:1,duration:animateDuration,delay:animateDelay}],easing:animateEasing})}$("body").on("click",".playAllAnimation",function(){var id=$(this).data("id")+"";var userBlock=_.findWhere(App.userBlocks,{id:id});var mode=$("#b_"+id).data("mode")||"l";$.each(userBlock.data.flexblocks,function(index,flexblock){playAnimation(flexblock,mode,id,index)})});$("body").on("click",".playAnimation",function(){var id=$(this).data("id")+"";var index=$(this).data("index")+"";var userBlock=
