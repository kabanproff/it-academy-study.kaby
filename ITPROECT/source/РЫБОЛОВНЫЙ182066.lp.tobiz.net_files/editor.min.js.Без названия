_.extend({},this_block_size_styles,data.flexblock.style["xxs"])}if(typeof style[size]=="undefined")style[size]={};if(typeof style[size][idx]=="undefined")style[size][idx]="";if(typeof frontStyle[size]=="undefined")frontStyle[size]={};if(typeof frontStyle[size][idx]=="undefined")frontStyle[size][idx]="";style[size][idx]+=`#b_${id} .section_inner { height: ${_.get(this_block_size_styles,`height`)}px }`+"\n";let styleWrapper="";let onlyFront="";let styleContent="";let styleHoverContent="";let styleFormBtn=
"";let styleHoverFormBtn="";$.each(this_size_styles,function(prop,value){switch(prop){case "invisible":if(_.get(this_size_styles,"invisible")=="1")onlyFront+=`display: none;`+"\n";else onlyFront+=`display: block;`+"\n";break;case "top":let topUnit="px";if(_.get(this_size_styles,"topUnit")=="%")topUnit="%";styleWrapper+=`top: ${value}${topUnit};`+"\n";break;case "left":let leftUnit="px";if(_.get(this_size_styles,"leftUnit")=="%")leftUnit="%";styleWrapper+=`left: ${value}${leftUnit};`+"\n";break;case "width":let widthUnit=
"px";if(_.get(this_size_styles,"widthUnit")=="%")widthUnit="%";styleWrapper+=`width: ${value}${widthUnit};`+"\n";break;case "height":let heightUnit="px";if(_.get(this_size_styles,"heightUnit")=="%")heightUnit="%";styleWrapper+=`height: ${value}${heightUnit};`+"\n";break;case "opacity":styleWrapper+=`opacity: ${value};`+"\n";break;case "zIndex":styleWrapper+=`z-index: ${value};`+"\n";break;case "animateType":onlyFront+=`--animate-type: ${value};`+"\n";if(value=="off"){onlyFront+=`opacity: 1;`+"\n";
break}if(value=="fadeIn"){onlyFront+=`opacity: 0;`+"\n";break}if(value=="fadeInLeft"){onlyFront+=`opacity: 0;`+"\n";break}if(value=="fadeInRight"){onlyFront+=`opacity: 0;`+"\n";break}if(value=="fadeInTop"){onlyFront+=`opacity: 0;`+"\n";break}if(value=="fadeInBottom"){onlyFront+=`opacity: 0;`+"\n";break}break;case "animateEvent":onlyFront+=`--animate-event: ${value};`+"\n";break;case "animateDelay":onlyFront+=`--animate-delay: ${value};`+"\n";break;case "animateDuration":onlyFront+=`--animate-duration: ${value};`+
"\n";break;case "animateEasing":onlyFront+=`--animate-easing: ${value};`+"\n";break;case "animateDistance":onlyFront+=`--animate-distance: ${value};`+"\n";break;case "animateZoom":onlyFront+=`--animate-zoom: ${value};`+"\n";break;case "animateRotate":onlyFront+=`--animate-rotate: ${value};`+"\n";break}switch(prop){case "backgroundColor":if(_.get(this_size_styles,"backgroundGradient")!="1")styleContent+=`background-color: ${value};`+"\n";break;case "backgroundGradient":let backgroundGradient=_.get(this_size_styles,
