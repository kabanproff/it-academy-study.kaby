"\n";break;case "fontFamily":let fontsList=value.split(",");let fontsListStr='"'+fontsList.join('", "')+'"';styleContent+=`font-family: ${fontsListStr};`+"\n";break}switch(prop){case "backgroundHoverColor":styleHoverContent+=`background-color: ${value};`+"\n";break}switch(prop){case "btnBackgroundColor":if(_.get(this_size_styles,"btnBackgroundGradient")!=1)styleFormBtn+=`background-color: ${value};`+"\n";break;case "btnBackgroundGradient":let btnBackgroundGradient=_.get(this_size_styles,"btnBackgroundGradient");
